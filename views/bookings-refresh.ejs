<% for (i=0; i<12; i++) { %>
<div class="col-3">
  <%

    if (bookings.indexOf(i+8) == -1) {
    if (currentUser) { %>
  <form id="<%= i %>" action="/rooms/<%= room.id %>/book/<%= i + 8 %>" method="POST">
    <button type="submit" class="time-slot"><%=  i + 8 + ":00" %></button>
  </form>
  <% } else { %>
  <a id="<%= room.id + (i + 8) %>" class="time-slot enabled"> <%=  i + 8 + ":00" %></a>
  <% }
  } else { %>
  <div class="time-slot disabled"><%=  i + 8 + ":00" %></div>
  <% } %>
</div>
<% } %>
